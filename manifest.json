{
  "name": "my first extention",
  "version": "1.0.0",
  "description": "My chrome extension",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistence": false
  },
  "content_scripts": [
    {
      "matches": ["https://app.slack.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["bookmarks"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["bookmarks"]
}
